Package: ProtVisDatabase
Type: Package
Title: Protein Visualization and Annotation Database for Arabidopsis thaliana
Version: 0.1.0
Authors@R: c(
    person(
      "Fei", "Liang",
      email = "1264284076@qq.com",
      role = c("aut", "cre"),
      comment = c(ORCID = "0009-0002-8775-2565")
    )
